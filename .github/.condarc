# From
# https://github.com/conda/conda/blob/aed799b56d9ee16a3653928527e2af6e41394e85/conda/base/context.py#L1132
local_repodata_ttl: 3600
repodata_fns: repodata.json
remote_connect_timeout_secs: 60
remote_read_timeout_secs: 120
remote_max_retries: 5
remote_backoff_factor: 2
show_channel_urls: true
use_index_cache: true
use_only_tar_bz2: true
unsatisfiable_hints_check_depth: 0
unsatisfiable_hints: false
